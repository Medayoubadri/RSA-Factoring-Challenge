#!/usr/bin/env python3
import sys
import math

def factorize_number(n):
    """Finds two factors of n."""
    if n <= 1:
        return n, 1
    for i in range(2, int(math.isqrt(n)) + 1):
        if n % i == 0:
            return i, n // i
    return n, 1

if __name__ == "__main__":
    if len(sys.argv) != 2:
        print(f"Usage: {sys.argv[0]} <file>")
        sys.exit(1)

    try:
        with open(sys.argv[1], "r") as file:
            for line in file:
                try:
                    num = int(line.strip())
                    if num > 1:
                        p, q = factorize_number(num)
                        print(f"{num}={p}*{q}")
                    else:
                        print(f"Error: {num} is not greater than 1.")
                except ValueError:
                    print(f"Error: Invalid number {line.strip()}.")
    except FileNotFoundError:
        print(f"Error: file {sys.argv[1]} not found.")
        sys.exit(1)

